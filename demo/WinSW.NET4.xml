<configuration>
    <!-- ID of the service. It should be unique accross the Windows system-->
    <id>RC-CONNECTOR-SERVICE</id>
    <!-- Display name of the service -->
    <name>Connector between Cornerstone and Sage</name>
    <!-- Service description -->
    <description>Cornerstone and Sage connector Service</description>

    <!-- we need to execute java -jar ./XXX.jar -->
    <executable>java</executable>
    <arguments>-jar "%BASE%\connector.jar</arguments>
</configuration>